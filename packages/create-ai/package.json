{
  "name": "create-ai",
  "version": "0.1.0",
  "description": "CLI to quickly scaffold new AI applications with starter templates.",
  "bin": {
    "create-ai": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "fs": [
        "node_modules/@types/node/fs.d.ts"
      ],
      "path": [
        "node_modules/@types/node/path.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "kleur": "^4.1.5",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.6",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.0.0"
  }
}
