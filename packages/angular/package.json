{
  "name": "@ai-toolkit/angular",
  "version": "2.0.45",
  "description": "Angular implementation of ai-toolkit.",
  "license": "Apache-2.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --tsconfig tsconfig.build.json --watch",
    "clean": "del-cli dist *.tsbuildinfo",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --build",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "vitest --config vitest.config.ts --run",
    "test:update": "vitest --config vitest.config.ts --run -u",
    "test:watch": "vitest --config vitest.config.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "CHANGELOG.md",
    "README.md"
  ],
  "dependencies": {
    "@ai-toolkit/provider-utils": "0.0.0",
    "ai": "workspace:*"
  },
  "devDependencies": {
    "@ai-toolkit/test-server": "0.0.0",
    "@types/node": "20.17.24",
    "@khulnasoft/ai-tsconfig": "workspace:*",
    "eslint": "8.57.1",
    "eslint-config-khulnasoft-ai": "workspace:*",
    "jsdom": "^24.0.0",
    "tsup": "^7.2.0",
    "typescript": "5.8.3",
    "vitest": "^3.0.0",
    "zod": "3.25.76"
  },
  "peerDependencies": {
    "@angular/core": ">=16.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
