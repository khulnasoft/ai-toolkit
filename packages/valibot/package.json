{
  "name": "@ai-toolkit/valibot",
  "version": "0.1.18",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "ai-toolkit": "2.0.0"
  },
  "devDependencies": {
    "@types/node": "20.17.24",
    "@khulnasoft/ai-tsconfig": "workspace:*",
    "tsup": "^8",
    "typescript": "5.6.3",
    "valibot": "^1.0.0-rc.0 || ^1.0.0"
  },
  "peerDependencies": {
    "@valibot/to-json-schema": "^1.0.0-rc.0 || ^1.0.0",
    "valibot": "^1.0.0-rc.0 || ^1.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://sdk.khulnasoft.com/docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khulnasoft/ai.git"
  },
  "bugs": {
    "url": "https://github.com/khulnasoft/ai/issues"
  },
  "keywords": [
    "ai-toolkit"
  ]
}
