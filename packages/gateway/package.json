{
  "name": "@ai-toolkit/gateway",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "internal/dist/**/*",
    "CHANGELOG.md",
    "internal.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist && rm -rf internal/dist",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "pnpm test:node && pnpm test:edge",
    "test:edge": "vitest --config vitest.edge.config.js --run",
    "test:node": "vitest --config vitest.node.config.js --run",
    "test:node:watch": "vitest --config vitest.node.config.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./internal": {
      "types": "./internal/dist/index.d.ts",
      "import": "./internal/dist/index.mjs",
      "module": "./internal/dist/index.mjs",
      "default": "./internal/dist/index.js"
    }
  },
  "dependencies": {
    "@ai-toolkit/provider": "workspace:*",
    "@ai-toolkit/provider-utils": "workspace:*"
  },
  "devDependencies": {
    "@ai-toolkit/eslint-config": "workspace:*",
    "@ai-toolkit/tsconfig": "workspace:*",
    "@types/node": "^18.19.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.0",
    "tsup": "^8.2.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
