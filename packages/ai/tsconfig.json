{
  "extends": "./node_modules/@khulnasoft/ai-tsconfig/base.json",
  "compilerOptions": {
    "target": "ES2018",
    "stripInternal": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "@types/node",
      "vitest/globals"
    ],
    "composite": false,
    "rootDir": ".",
    "outDir": "dist",
    "jsx": "react-jsx"
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "dist",
    "build",
    "node_modules",
    "tsup.config.ts",
    "internal.d.ts",
    "mcp-stdio.d.ts",
    "test.d.ts"
  ],
  "references": [
    {
      "path": "../provider"
    },
    {
      "path": "../provider-utils"
    }
  ]
}